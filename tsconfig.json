{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2021",
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
  },
  "exclude": ["dist", "*.d.ts"]
}
